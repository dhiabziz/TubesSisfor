<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Document Specific Groups -->
    <record id="group_document_viewer" model="res.groups">
        <field name="name">Document Viewer</field>
        <field name="category_id" ref="base.module_category_project"/>
        <field name="implied_ids" eval="[(4, ref('access_control.group_project_client'))]"/>
    </record>

    <record id="group_document_uploader" model="res.groups">
        <field name="name">Document Uploader</field>
        <field name="category_id" ref="base.module_category_project"/>
        <field name="implied_ids" eval="[
            (4, ref('group_document_viewer')),
            (4, ref('access_control.group_project_member'))
        ]"/>
    </record>
</odoo>